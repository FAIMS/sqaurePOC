<?xml version='1.0' encoding='utf-8'?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>FILL THIS IN</h:title>
    <model>
      <instance>
        <faims id="FILL_THIS_IN">
          <!--START: Style-->
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <!--END: Style-->
          <User_List>
            <User_List>
              <User_List/>
            </User_List>
          </User_List>
          <Control>
            <Control>
              <New_Square/>
              <GPS_diagnostics/>
            </Control>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_List/>
            </Search>
            <Autonumbering>
              <Next_Square_ID/>
            </Autonumbering>
          </Control>
          <Square>
            <General>
              <Square_ID/>
              <Width/>
              <Colgroup_1>
                <Col_1>
                  <Latitude/>
                </Col_1>
                <Col_2>
                  <Longitude/>
                </Col_2>
              </Colgroup_1>
              <Colgroup_2>
                <Col_1>
                  <Northing/>
                </Col_1>
                <Col_2>
                  <Easting/>
                </Col_2>
                <Col_3>
                  <Accuracy/>
                </Col_3>
              </Colgroup_2>
              <Take_From_GPS_1/>
              <Start_Line/>
              <Northing1/>
              <Easting1/>
              <Stop_Line/>
              <Northing2/>
              <Easting2/>
            </General>
          </Square>
        </faims>
      </instance>
      <bind nodeset="/faims/Control/Autonumbering/Next_Square_ID" type="decimal"/>
      <bind nodeset="/faims/Square/General/Width" type="decimal"/>
      <bind nodeset="/faims/Square/General/Northing1" type="decimal"/>
      <bind nodeset="/faims/Square/General/Easting1" type="decimal"/>
      <bind nodeset="/faims/Square/General/Northing2" type="decimal"/>
      <bind nodeset="/faims/Square/General/Easting2" type="decimal"/>
    </model>
  </h:head>
  <h:body>
    <!--START: Style-->
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <!--END: Style-->
    <group ref="User_List">
      <label>{User_List}</label>
      <group ref="User_List" faims_scrollable="false">
        <label>{User_List}</label>
        <select1 appearance="compact" ref="User_List">
          <label/>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Control">
      <label>{Control}</label>
      <group ref="Control" faims_scrollable="false">
        <label>{Control}</label>
        <trigger ref="New_Square">
          <label>{New_Square}</label>
        </trigger>
        <input faims_read_only="true" ref="GPS_diagnostics">
          <label>{GPS_diagnostics}</label>
        </input>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{Search}</label>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="large">
            <label/>
            <trigger ref="Search_Button">
              <label>{Search}</label>
            </trigger>
          </group>
        </group>
        <select1 appearance="compact" ref="Entity_List">
          <label>{Entity_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Autonumbering">
        <label>{Autonumbering}</label>
        <input ref="Next_Square_ID" faims_style_class="required">
          <label>{Next_Square_ID}</label>
        </input>
      </group>
    </group>
    <group ref="Square" faims_archent_type="Square">
      <label>{Square}</label>
      <group ref="General">
        <label>{General}</label>
        <input faims_attribute_name="Square ID" faims_attribute_type="measure" ref="Square_ID" faims_certainty="false">
          <label>{Square_ID}</label>
        </input>
        <input faims_attribute_name="Width" faims_attribute_type="measure" ref="Width" faims_annotation="false" faims_certainty="false">
          <label>{Width}</label>
        </input>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Latitude" faims_attribute_type="measure" ref="Latitude" faims_style_class="required" faims_read_only="true">
              <label>{Latitude}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Longitude" faims_attribute_type="measure" ref="Longitude" faims_style_class="required" faims_read_only="true">
              <label>{Longitude}</label>
            </input>
          </group>
        </group>
        <group ref="Colgroup_2" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Northing" faims_attribute_type="measure" ref="Northing" faims_style_class="required" faims_read_only="true">
              <label>{Northing}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Easting" faims_attribute_type="measure" ref="Easting" faims_style_class="required" faims_read_only="true">
              <label>{Easting}</label>
            </input>
          </group>
          <group ref="Col_3" faims_style="even">
            <label/>
            <input faims_attribute_name="Accuracy" faims_attribute_type="measure" ref="Accuracy" faims_style_class="required" faims_read_only="true">
              <label>{Accuracy}</label>
            </input>
          </group>
        </group>
        <trigger ref="Take_From_GPS_1">
          <label>{Take_From_GPS}</label>
        </trigger>
        <trigger ref="Start_Line">
          <label>{Start_Line}</label>
        </trigger>
        <input faims_attribute_name="Northing1" faims_attribute_type="measure" ref="Northing1">
          <label>{Northing1}</label>
        </input>
        <input faims_attribute_name="Easting1" faims_attribute_type="measure" ref="Easting1">
          <label>{Easting1}</label>
        </input>
        <trigger ref="Stop_Line">
          <label>{Stop_Line}</label>
        </trigger>
        <input faims_attribute_name="Northing2" faims_attribute_type="measure" ref="Northing2">
          <label>{Northing2}</label>
        </input>
        <input faims_attribute_name="Easting2" faims_attribute_type="measure" ref="Easting2">
          <label>{Easting2}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
